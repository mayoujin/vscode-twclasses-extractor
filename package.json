{
  "name": "tw-classes-extractor",
  "displayName": "twClassesExtractor",
  "description": "Extracting tw classes from HTML and generate CSS stylesheet",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mayoujin/vscode-ecsstractor.git"
  },
  "publisher": "mayoujin",
  "engines": {
    "vscode": "^1.46.0",
    "node": ">=14.4.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.twclassesextractor.run"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.twclassesextractor.run",
        "title": "TailwindCss: Extract classes from HTML"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "format": "prettier --write ./src/**/*.{js,ts}",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/glob": "7.1.3",
    "@types/domhandler": "^2.4.1",
    "@types/htmlparser2": "3.10.2",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.10",
    "@types/prettier": "2.1.5",
    "@types/vscode": "1.51.0",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "ava": "3.13.0",
    "eslint": "7.15.0",
    "glob": "7.1.6",
    "mocha": "8.2.0",
    "semantic-release": "17.3.0",
    "semantic-release-vsce": "3.0.1",
    "typescript": "4.0.3",
    "vscode": "1.1.37",
    "vscode-test": "1.4.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "css-to-tailwind": "^1.0.3",
    "html-tags": "^3.1.0",
    "htmlparser2": "^5.0.1",
    "option-t": "^24.0.0",
    "p-pipe": "^3.1.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.1"
  }
}
